<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <RootNamespace>Arc4u.Serializer.Json</RootNamespace>
    <Description>Cross platform JSon serializer.</Description>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <Version>9.9.9.9</Version>
    <Authors>Gilles Flisch</Authors>
    <Company>Gilles Flisch</Company>
    <Copyright>Gilles Flisch</Copyright>
    <PackageProjectUrl>https://github.com/GFlisch/Arc4u</PackageProjectUrl>
    <RepositoryUrl>https://arc4u.visualstudio.com/Arc4u/_git/Standard</RepositoryUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Arc4u</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <UserSecretsId>3ecd0e40-959a-4d12-9006-ea8e522d4025</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="5.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Arc4u.Standard.Diagnostics\Arc4u.Standard.Diagnostics.csproj" />
    <ProjectReference Include="..\Arc4u.Standard.Serializer\Arc4u.Standard.Serializer.csproj" />
  </ItemGroup>
  
</Project>
